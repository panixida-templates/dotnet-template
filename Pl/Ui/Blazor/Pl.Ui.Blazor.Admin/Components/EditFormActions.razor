<MudStack Row Justify="Justify.FlexEnd" Spacing="2" Class="@Class">
    <MudButton Variant="Variant.Text" ButtonType="ButtonType.Button" OnClick="OnCancel" Disabled="Disabled">
        @CancelText
    </MudButton>

    <MudButton Variant="Variant.Filled" Color="Color.Primary" StartIcon="@Icons.Material.Filled.Save" ButtonType="ButtonType.Submit" Disabled="Disabled">
        @SubmitText
    </MudButton>
</MudStack>
